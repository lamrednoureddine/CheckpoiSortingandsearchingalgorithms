program InsertionSort2;

procedure InsertionSort(var arr: array of Integer);
var
  n, i, j, current_element: Integer;
begin
  n := Length(arr);
  for i := 1 to n - 1 do
  begin
    current_element := arr[i];
    j := i;
    while (j > 0) and (current_element < arr[j - 1]) do
    begin
      arr[j] := arr[j - 1];
      j := j - 1;
    end;
    arr[j] := current_element;
  end;
end;

